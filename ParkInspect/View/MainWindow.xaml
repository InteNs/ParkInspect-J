<Window x:Class="ParkInspect.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ParkInspect"
        xmlns:view="clr-namespace:ParkInspect.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525"
        WindowStartupLocation="CenterScreen"
        DataContext="{Binding Main, Source={StaticResource Locator}}">
    <StackPanel>
        <Button Command="{Binding Router.SetViewCommand, Source={StaticResource Locator} }" CommandParameter="test-1">Test1 Items</Button>
        <Button Command="{Binding Router.SetViewCommand, Source={StaticResource Locator} }" CommandParameter="test-2">Test2 Items</Button>
        <Button Command="{Binding Router.SetViewCommand, Source={StaticResource Locator} }" CommandParameter="test-3">Add a commission</Button>
        <ContentControl Content="{Binding Router.CurrentView, Source={StaticResource Locator}}" />
    </StackPanel>
</Window>
